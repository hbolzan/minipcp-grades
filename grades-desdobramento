(def itens-teste
  [
    {:tipo 4 :codigo "PA-01-C2:0002.0010" :quantidade 10}
    {:tipo 4 :codigo "PA-0002:0020.0016" :quantidade 4}
    {:tipo 4 :codigo "PA-01-C2:0004.0012" :quantidade 20}
    {:tipo 4 :codigo "PA-0002:0006.0012" :quantidade 18}
    {:tipo 4 :codigo "PA-01-C2:0024.0010" :quantidade 12}
    {:tipo 4 :codigo "PA-01-C2:0008.0016" :quantidade 15}
    {:tipo 4 :codigo "PA-0002:0020.0016" :quantidade 8}
    ])
;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/itens-teste</span>","value":"#'user/itens-teste"}
;; <=

;; @@
; (clojure.pprint/pprint (items-to-grid dados itens-teste))
; (cell-to-item {"0006.0012" 10} {:desdobramento "0006.0012", :quantidade 18})

; (map (fn [e] e) {"0006.0012" 18, "0020.0016" 12})
; (vals {"0006.0012" 18, "0020.0016" 12})
; (hash-map :codigo (str "PA-0002" ":" (first (keys {"0006.0012" 18}))) :quantidade (first (vals {"0006.0012" 18})))

; (def grid (first (items-to-grid dados itens-teste)))

; (grid-to-items-map grid)
(clojure.pprint/pprint (grids-to-items (items-to-grid dados itens-teste)))
