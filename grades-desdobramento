;; @@
(defn map-to-kwmap [m]
  (into {} (map #(vector (keyword (first %)) (last %))) m))


(defn tratar-grade [grade]
  (cons
    (first grade)
    (map 
      (fn [linha] (cons (first linha) (map map-to-kwmap (rest linha)))) 
      (rest grade))))

(defn tratar-grid [grid]
  (let [grade (:grade grid)] 
    (assoc grid :grade (tratar-grade grade))))


;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/tratar-grid</span>","value":"#'user/tratar-grid"}
;; <=

;; @@
; (map (fn [e] e) (rest (:grade (first grades-teste))))
(tratar-grid (first grades-teste))
