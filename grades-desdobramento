;; @@
(defn find-grade [tipo codigo grades]
  (find-first 
    (fn [grade] (let [produto (:produto grade)] (and (= tipo (:tipo produto)) (= codigo (:codigo produto)))))
    grades))

;; @@
;; =>
;;; {"type":"html","content":"<span class='clj-var'>#&#x27;user/find-grade</span>","value":"#'user/find-grade"}
;; <=

